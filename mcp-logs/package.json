{
  "name": "lemarsyn-mcp-logs",
  "version": "0.1.0",
  "private": true,
  "description": "MCP server for LemarSyn service logs and dev-cli passthrough.",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "build": "rm -rf dist && esbuild src/server.ts --bundle --platform=node --format=cjs --target=node18 --outfile=dist/server.js",
    "dev": "tsx src/server.ts",
    "start": "node dist/server.js",
    "test": "tsx --test test/**/*.test.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "tsx": "^4.20.4",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18"
  }
}
